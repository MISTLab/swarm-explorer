alias ptpstatus='sudo systemctl status ptp4l'
alias ptprestart='systemctl daemon-reload; sudo systemctl restart ptp4l'
alias lidar='ptprestart; roslaunch ouster_ros ouster.launch sensor_hostname:=os-992039000794.local udp_dest:=169.254.8.70 timestamp_mode:=TIME_FROM_PTP_1588'
alias cameraswheel='roslaunch realsense2_camera rs_d400_and_t265.launch topic_odom_in:=/rr_openrover_driver/odom_encoder calib_odom_file:=/home/rover/swarm-explorer/robot/config/wheelodom.json'
alias cameras='roslaunch realsense2_camera rs_d400_and_t265.launch'
alias explorer='docker run --net=host -it lajoiepy/swarm-explorer:arm64'
alias roverstart='roslaunch rr_openrover_driver rover_no_teleop.launch'
alias entercontainer='docker exec -it $(docker container ls -q) /bin/bash'
alias stop='rostopic pub /soft_estop/enable std_msgs/Bool "data: true" -1'
alias restart='rostopic pub /soft_estop/reset std_msgs/Bool "data: true" -1'
alias exposureoff='rosrun dynamic_reconfigure dynparam set /d400/rgb_camera enable_auto_exposure 0 && rosrun dynamic_reconfigure dynparam set /d400/rgb_camera enable_auto_white_balance 0'
